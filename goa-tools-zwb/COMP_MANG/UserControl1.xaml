<Window x:Class="COMP_MANG.MyWindow"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             mc:Ignorable="d" 
             Height="480" Width="210"
             ResizeMode="NoResize">
    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition Height="25"/>
            <RowDefinition Height="400"/>
            <RowDefinition Height="25"/>
        </Grid.RowDefinitions>
        <Label Content="请选择要删除图元的标高" Grid.Row="0" HorizontalAlignment="Left"  VerticalAlignment="Top" Width="200" Height="25"/>
        <ListBox x:Name="listbox" Grid.Row="1" HorizontalAlignment="Left" Height="400"  VerticalAlignment="Top" Width="200" SelectionMode="Extended"/>
        <Button x:Name="button" Grid.Row="2" Content="确定" HorizontalAlignment="Left"  VerticalAlignment="Top" Width="200" Height="25" Click="button_Click"/>

    </Grid>
</Window>
